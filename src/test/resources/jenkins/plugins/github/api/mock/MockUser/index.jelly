<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler"><st:contentType value="application/json; charset=utf-8"/>{
  "login": ${app.json(it.login)},
  "id": ${it.id},
  "avatar_url": ${app.json(it.avatarUrl)},
  "gravatar_id": "",
  "url": "${app.url}/users/${app.pathSegment(it.login)}",
  "html_url": "https://github.com/${app.pathSegment(it.login)}",
  "followers_url": "${app.url}/users/${app.pathSegment(it.login)}/followers",
  "following_url": "${app.url}/users/${app.pathSegment(it.login)}/following{/other_user}",
  "gists_url": "${app.url}/users/${app.pathSegment(it.login)}/gists{/gist_id}",
  "starred_url": "${app.url}/users/${app.pathSegment(it.login)}/starred{/owner}{/repo}",
  "subscriptions_url": "${app.url}/users/${app.pathSegment(it.login)}/subscriptions",
  "organizations_url": "${app.url}/users/${app.pathSegment(it.login)}/orgs",
  "repos_url": "${app.url}/users/${app.pathSegment(it.login)}/repos",
  "events_url": "${app.url}/${app.pathSegment(it.login)}/events{/privacy}",
  "received_events_url": "${app.url}/users/${app.pathSegment(it.login)}/received_events",
  "type": "${it.type}",
  "site_admin": ${app.json(it.siteAdmin)},
  "name": ${app.json(it.name)},
  "company": ${app.json(it.company)},
  "blog": ${app.json(it.blog)},
  "location": ${app.json(it.location)},
  "email": ${app.json(it.email)},
  "hireable": ${it.hireable},
  "bio": ${app.json(it.bio)},
  "public_repos": ${it.publicRepos},
  "public_gists": 0,
  "followers": ${it.followers},
  "following": ${it.following},
  "created_at": "${app.tz(it.created)}",
  "updated_at": "${app.tz(it.updated)}"
}
</j:jelly>
